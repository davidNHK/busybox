{
  "author": "David Ng <david.ng.dev@gmail.com>",
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.4",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-lerna-scopes": "9.1.2",
    "commitiquette": "1.0.7",
    "commitizen": "4.1.2",
    "eslint": "7.7.0",
    "husky": "4.2.5",
    "lerna": "3.22.1",
    "lint-staged": "10.2.11",
    "npm-package-json-lint": "5.1.0",
    "prettier": "2.0.5",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lerna run test",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "license": "MIT",
  "name": "busybox",
  "private": true,
  "repository": "git@github.com:davidNHK/busybox.git",
  "scripts": {
    "build": "yarn lerna run build",
    "eslint": "eslint --fix --ext=ts,js,json",
    "lint:js": "yarn eslint .",
    "lint:package-json": "yarn npmPkgJsonLint .",
    "test": "yarn lerna run test"
  },
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ]
}
